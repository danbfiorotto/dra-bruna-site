{
  "name": "dra-bruna-site",
  "version": "1.0.0",
  "description": "Site institucional Dra. Bruna - Web Vitrine",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/danbfiorotto/dra-bruna-site.git"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "tailwindcss",
    "endodontia",
    "dentist",
    "medical"
  ],
  "author": "Dra. Bruna Torelli Soares",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:github": "copy next.config.github.js next.config.js && next build",
    "build:vercel": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rmdir /s /q .next out dist 2>nul || echo Cleaned",
    "deploy:github": "npm run build:github && git add out/ && git commit -m \"Deploy: Update site\" && git push origin main",
    "deploy:vercel": "npm run build:vercel && vercel --prod"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@supabase/supabase-js": "^2.57.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "critters": "^0.0.23",
    "lucide-react": "^0.323.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
